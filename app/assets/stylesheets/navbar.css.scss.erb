#home-navbar {
  height: 50px;
  width: 100%;
  line-height: 60px;
  background-color: #fff;
  font-weight: 600;
  padding-left: 35px;
  padding-right: 35px;
  box-shadow: 0px 2px 4px rgba(191, 191, 191, 0.5);
  position: fixed;
  top: 0px;
  z-index: 2;

  .menu-icon {
    display: none;
    cursor: pointer;

    div {
      width: 30px;
      height: 4px;
      border-radius: 2px;
      background-color: #4a4a4a;

      &:nth-child(2) {
        margin-top: 4px;
        margin-bottom: 4px;
      }
    }
  }

  .back {
    text-align: center;
    color: #00C39C;
    float: left;
    position: relative;
    top: -10px;

    img {
    height: 30px;
    width: auto;
  }
  }

  .left {
    float: left;
  }

  #st{
      display: block;
      position: relative;
      top: 8px;
      right: -30px;
      width: 375px;
      height: 30px;
      padding:6px 15px 6px 30px;
      margin:3px;
      background: url('search.png') no-repeat 8px 6px;
      background-color: #FDFDFD;
      border: 1px solid #D4D4D4;
      color: #888;
      font-weight: 400;
  }

  #st::-webkit-input-placeholder { /* WebKit browsers */
    color:    #aaa;
  }

  #st:-moz-placeholder { /* Mozilla Firefox 4 to 18 */
     color:    #aaa;
     opacity:  1;
  }

  #st::-moz-placeholder { /* Mozilla Firefox 19+ */
     color:    #aaa;
     opacity:  1;
  }

  #st:-ms-input-placeholder { /* Internet Explorer 10+ */
     color:    #aaa;
  }

  .right {
    float: right;
  }

  .nav-user-circle {
    width: 35px;
    height: 35px;
    border-radius: 50%;
    background-color: #00c39c;
    display: inline-block;
    color: #fff;
    line-height: 35px;
    text-align: center;
  }

  .logo-container {
    display: none;

    .nav-logo {
      max-height: 30px;
    }
  }


  .nav-links {
    float: right;
    margin-bottom: 0px;

    li {
      list-style-type: none;
      display: inline-block;
      margin-right: 10px;
      z-index: 1000;

      .nav-link {
        color: #4a4a4a;
        font-weight: 600;
        font-size: 15px;
      }

      .nav-link:hover {
        opacity: 0.8;
      }
    }
  }
}
