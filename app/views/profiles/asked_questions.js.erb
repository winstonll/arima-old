$(document).ready(function() {
  // $(".questions-list").html('<%= escape_javascript(render partial: "profiles/asked_questions") %>')

    <% if @user && @unanswered.any? %>
    $("#questions-list").html('<%= escape_javascript(render partial: "asked_questions") %>');
  <% else %>
    $("#questions-list").html('<%= escape_javascript(render partial: "answered_questions") %>');
  <% end %>

  <% if @user && !@answered.empty? %>
    <%= render partial: "asked_questions" %>
  <% end %>
})